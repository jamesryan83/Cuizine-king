>\r\n<div id=\"store-menu-list\"></div>\r\n\r\n\r\n<!-- A single store menu heading item -->\r\n<script id=\"template-store-menu-heading\" type=\"text/html\">\r\n    <div class=\"store-menu-list-item heading\">\r\n        <h4 data-content=\"title\"></h4>\r\n        <hr class=\"hr-1\" />\r\n    </div>\r\n</script>\r\n\r\n\r\n<!-- A single store menu item -->\r\n<script id=\"template-store-menu-item\" type=\"text/html\">\r\n    <div class=\"store-menu-list-item\">\r\n\r\n        <!-- Details panel -->\r\n        <div class=\"store-menu-list-item-details\">\r\n            <div class=\"store-menu-list-item-content\">\r\n                <h4 data-content=\"name\"></h4>\r\n                <label data-content=\"lowestOptionPrice\" data-format=\"lowestOptionPriceFormatter\"></label>\r\n                <p data-content=\"description\"></p>\r\n                <label data-class=\"class1\">GLUTEN FREE</label>\r\n                <label data-class=\"class2\">VEGETARIAN</label>\r\n                <label data-class=\"class3\">NO DELIVERY</label>\r\n            </div>\r\n        </div>\r\n\r\n        <!-- Options panel -->\r\n        <div class=\"store-menu-list-item-options\">\r\n            <div class=\"store-menu-list-item-button store-menu-list-item-options-cancel\"><label class=\"center noselect\">CANCEL</label></div>\r\n            <div class=\"store-menu-list-item-content clearfix\"></div>\r\n        </div>\r\n    </div>\r\n</script>\r\n\r\n\r\n<!-- A single store menu item option -->\r\n<script id=\"template-store-menu-option\" type=\"text/html\">\r\n    <div class=\"store-menu-list-item-option\">\r\n        <div class=\"center\">\r\n            <label data-content=\"name\"></label>\r\n            <label data-content=\"price\" data-format=\"priceFormatter\"></label>\r\n        </div>\r\n    </div>\r\n</script>\r\n\r\n\r\n\r\n        <!-- Edit menu items -->\r\n        <div id=\"edit-menu-items\">\r\n            <div id=\"edit-menu-items-list\">\r\n\r\n            </div>\r\n        </div>\r\n\r\n\r\n        <!-- Footer -->\r\n        <div id=\"store-footer\">\r\n            <p id=\"store-disclaimer\"></p>\r\n        </div>\r\n\r\n    </div>\r\n\r\n\r\n    <!-- floating buttons -->\r\n    <div class=\"page-cms-floating-buttons double-height\">\r\n        <div>\r\n            <button id=\"cms-menu-add-category\" class=\"button-3\">ADD CATEGORY</button>\r\n            <button id=\"cms-menu-add-menu-item\" class=\"button-3\">ADD MENU ITEM</button>\r\n        </div>\r\n        <div>\r\n            <label id=\"page-cms-menu-save\" class=\"label-button\">SAVE</label>\r\n            <button class=\"cms-menu-preview button-3\">PREVIEW</button>\r\n            <button class=\"cms-menu-return button-3\" style=\"display: none;\">RETURN</button>\r\n        </div>\r\n    </div>\r\n\r\n\r\n    <!-- preview border -->\r\n    <div id=\"preview-mode-border\" style=\"display: none;\">\r\n        <label>PREVIEW</label>\r\n    </div>\r\n\r\n</div>\r\n\r\n\r\n<!-- A single edit menu heading item -->\r\n<script type=\"text/html\" id=\"template-edit-menu-item-heading\">\r\n    <div class=\"edit-menu-item edit-menu-item-heading\">\r\n        <label>HEADING</label>\r\n        <label class=\"edit-menu-item-label\" data-content=\"title\"></label>\r\n        <input class=\"edit-menu-item-input\" class=\"input-1\" data-value=\"title\" />\r\n        <div class=\"edit-menu-item-delete\"></div>\r\n    </div>\r\n</script>\r\n\r\n\r\n<!-- A single edit menu product item -->\r\n<script type=\"text/html\" id=\"template-edit-menu-item-product\">\r\n    <div class=\"edit-menu-item edit-menu-item-product\">\r\n        <label>PRODUCT</label>\r\n        <label data-content=\"name\"></label>\r\n        <div class=\"edit-menu-item-delete\"></div>\r\n    </div>\r\n</script>\r\n\r\n\r\n<!--\r\n<script type=\"text/html\">\r\n    <div id=\"menu-edit-add-category\">\r\n        <label>Category Name</label>\r\n        <input id=\"menu-edit-category-name\" />\r\n    </div>\r\n\r\n    <div i